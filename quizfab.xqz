          <singleAns>True</singleAns>
          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">Rifampicin</choice>
          <choice correct="True" TextFormat="1" pointsIn="1" pointsOut="0">Erythromycin</choice>
          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">Paracetamol</choice>
          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">All</choice>
        </answers>
      </question>
      <question type="1" category="Standard" include="True" weight="1" timetoanswer="0" textFormat="1" sortAns="False" putSeparator="True">
        <text>Clearance is useful in calculation of</text>
        <answers type="1">
          <singleAns>False</singleAns>
          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">Bioavailability</choice>
          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">Loading dose.</choice>
          <choice correct="True" TextFormat="1" pointsIn="1" pointsOut="0">Half life</choice>
          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">All</choice>
        </answers>
      </question>
      <question type="1" category="Standard" include="True" weight="1" timetoanswer="0" textFormat="1" sortAns="False" putSeparator="True">
        <text>Lipophilicity reduces</text>
        <answers type="1">
          <singleAns>False</singleAns>
          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">Absorption</choice>
          <choice correct="True" TextFormat="1" pointsIn="1" pointsOut="0">Volume of distribution (Vd)</choice>
