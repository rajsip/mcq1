          <choice correct="False" TextFormat="1" pointsIn="1" pointsOut="0">All</choice>
        </answers>
      </question>
      <question type="1" category="Standard" include="True" weight="1" timetoanswer="0" textFormat="1" sortAns="False" putSeparator="True">
